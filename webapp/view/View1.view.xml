<mvc:View xmlns:html="http://www.w3.org/1999/xhtml" xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="br.com.demorouting.DemoRouting.controller.View1" displayBlock="true">
	<App id="idAppControl">
		<pages>
			<Page title="View 1">
				<content>
					
					<core:Fragment fragmentName="br.com.demorouting.DemoRouting.view.Newfragment" type="XML"></core:Fragment>
				    <List noDataText="Drop list items here" id="__list0" items="{/Products}">
				        <items>
				            <StandardListItem type="Navigation" title="{ProductName}" description="{QuantityPerUnit}" press="handleListItemPress" icon="sap-icon://picture" id="__item0"/>
				        </items>
				    </List>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>