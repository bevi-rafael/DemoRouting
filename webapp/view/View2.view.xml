<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
	controllerName="br.com.demorouting.DemoRouting.controller.View2" xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page title="View 2" showNavButton="true" navButtonPress="onNavBack">
				<content>
					<core:Fragment fragmentName="br.com.demorouting.DemoRouting.view.Newfragment" type="XML"></core:Fragment>
					
					<mvc:XMLView viewName="br.com.demorouting.DemoRouting.view.NestedView"></mvc:XMLView>
					
					<VBox>
						<Text text="{ProductName}"></Text>
						<Text text="{UnitPrice}"></Text>
						<Text text="{QuantityPerUnit}"></Text>
						<Text text="{UnitsInStock}"></Text>
					</VBox>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>